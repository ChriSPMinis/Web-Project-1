    <div class="container">
      <div class="row">
        <h2>Add Program</h2>
        <br>
        <form method="post" action="/programs/add">
            <fieldset>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                         <label for="program">Program Code:</label>
                         <input class="form-control" id="programCode" name="programCode" type="text">
                     </div>
                </div>
                  <div class="col-md-12">
                     <div class="form-group">
                          <label for="program">Program Name:</label>
                          <input class="form-control" id="programName" name="programName" type="text">
                       </div>
                    </div>          
            </fieldset>
                <input type="submit" class="btn btn-primary pullright" value="Add Program">
            </form>
        </div>
    </div>


